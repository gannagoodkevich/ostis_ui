sc_agent_of_printing_relevant_subject_area
=> nrel_main_idtf:
	[агентная scp-программа подсчета количества безуспешных запросов] (* <- lang_ru;; *);
	[agent scp-program to counting unsuccessfully questions] (* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_printing_relevant_subject_area_condition => ..sc_agent_of_printing_relevant_subject_area_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_to_print_relevant_subject_area
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_printing_relevant_subject_area
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_printing_relevant_subject_area
		};;		

	-> sc_agent_of_printing_relevant_subject_area_scp    
		(* <- active_sc_agent;; *);;  
	*);;

..sc_agent_of_printing_relevant_subject_area_condition
= [*
	action_to_print_relevant_subject_area _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_of_printing_relevant_subject_area_result
= [*
	action_to_print_relevant_subject_area _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
